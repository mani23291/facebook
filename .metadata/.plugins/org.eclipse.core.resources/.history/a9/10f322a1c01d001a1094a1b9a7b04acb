package basic;

public class string121219 {
        public static void main(String[] args) {
        	{
        	String a="manikandan";	
        	char w[]=a.toCharArray();
        	for(int p=0;p>w.length;p++)
        	{
        		System.out.print(w[p]);
      }
        	System.out.println();
        	System.out.println("print the reverse word:");
        	for(int p=w.length-1;p>-1;p--)
        	{
        		System.out.println(w[p]);
        	}
        		
		}
      }
 }
