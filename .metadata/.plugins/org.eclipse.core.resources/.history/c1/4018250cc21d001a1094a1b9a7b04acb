package basic;


import java.util.Scanner;

public class scanner91219 {
 public static void main(String[] args) {
	   int cutoff;
         String n;
		 Scanner ww=new Scanner(System.in);
		 System.out.println("enter the option");
	 n= ww.next();
		  switch(n) {
		  
		  case "A":
			  int a,b,c;
			  System.out.println("welcome to the SRMUNIVERSITY ");
			  System.out.print("10th mark");
		   a=ww.nextInt();
		   System.out.print("12th mark");
		   b=ww.nextInt();
		   System.out.print("polytechnic mark");
		   c=ww.nextInt();
		   cutoff=(a/4)+(b/4)+(c/2);
		   System.out.println("cutoff percentage"+cutoff);
		   if(cutoff>=60 && cutoff<=120)
		   {
		      System.out.println("can apply Polytechnic");
		   } 
		   else if(cutoff>=121&&cutoff<=160) {
		    	  System.out.println("can apply for engg");
		      }
		   else if(cutoff>=161&&cutoff<=200) {
			   System.out.println("can apply for medicin");
		   }
					
		   break;
		  }
 }
}